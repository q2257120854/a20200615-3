.grace-stars {
    display: flex;
    flex-wrap: nowrap;
}

.grace-stars-items {
    display: inline-block;
    text-align: center;
    line-height: 1.6em;
}

.grace-star-icon {
    font-family: "grace-iconfont"!important;
    font-size: 60rpx;
    font-style: normal;
}

.grace-star-icon:before {
    content: "\e634";
}
.grace-popup-menu {
    background: #FFFFFF;
    width: 258rpx;
    padding: 10rpx;
    right: 0px;
    top: 0px;
    position: absolute;
    z-index: 9999;
}

.grace-popup-mask {
    background: rgba(0,0,0,0.3);
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9998;
}